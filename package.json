{
  "name": "passkey-auth-plugin",
  "version": "1.1.0",
  "description": "Plugin para integrar passkeys (WebAuthn) como forma de autenticación sin contraseña",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "example:server": "node examples/server.js",
    "example:demo": "cd examples/demo && python3 -m http.server 8080"
  },
  "keywords": [
    "passkeys",
    "webauthn",
    "authentication",
    "security",
    "biometric",
    "passwordless"
  ],
  "author": "eyrockscript",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eyrockscript/passkey-auth-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/eyrockscript/passkey-auth-plugin/issues"
  },
  "homepage": "https://github.com/eyrockscript/passkey-auth-plugin#readme",
  "dependencies": {
    "@simplewebauthn/server": "^13.2.2",
    "@simplewebauthn/browser": "^13.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "typescript": "^5.9.3",
    "jest": "^30.2.0",
    "@types/jest": "^30.0.0",
    "ts-jest": "^29.2.5",
    "express": "^4.21.2",
    "@types/express": "^4.17.25",
    "cors": "^2.8.5",
    "@types/cors": "^2.8.17",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "files": [
    "dist",
    "examples",
    "README.md",
    "README.es.md",
    "DATABASE_STORES.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "overrides": {
    "test-exclude": "^7.0.1",
    "glob": "^10.4.5"
  }
}