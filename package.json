{
  "name": "passkey-auth-plugin",
  "version": "1.0.0",
  "description": "Plugin para integrar passkeys (WebAuthn) como forma de autenticación sin contraseña",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "example:server": "node examples/server.js",
    "example:demo": "cd examples/demo && python3 -m http.server 8080"
  },
  "keywords": [
    "passkeys",
    "webauthn",
    "authentication",
    "security",
    "biometric",
    "passwordless"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuusuario/passkey-auth-plugin.git"
  },
  "dependencies": {
    "@simplewebauthn/server": "^10.0.1",
    "@simplewebauthn/browser": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^30.0.5",
    "@types/jest": "^30.0.0",
    "express": "^4.18.0",
    "@types/express": "^4.17.0"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "files": [
    "dist",
    "examples",
    "README.md"
  ],
  "overrides": {
    "test-exclude": "^7.0.1",
    "glob": "^10.4.5"
  }
}